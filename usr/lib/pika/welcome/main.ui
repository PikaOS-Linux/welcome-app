          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
          </packing>
        </child>
      </object>
    </child>
    <child type="titlebar">
      <object class="GtkHeaderBar" id="main_Header">
        <property name="can-focus">False</property>
        <property name="show-close-button">True</property>
        <property name="subtitle" translatable="yes">Welcome to PikaOS the Cool birb OS</property>
        <property name="title" translatable="yes">PikaOS Welcome</property>
        <property name="visible">True</property>
        <child>
          <object class="GtkCheckButton" id="startup_switch">
            <property name="can-focus">True</property>
            <property name="draw-indicator">True</property>
            <property name="label" translatable="yes">Show on startup</property>
            <property name="receives-default">False</property>
            <property name="visible">True</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
